#include<stdio.h>
void main()
{
    char a[10];
    char b[10];
    char c[10];
    printf("Enter name1:");
    scanf("%s",&a);
    printf("Enter name2:");
    scanf("%s",&b);
    printf("Enter name3:");
    scanf("%s",&c);
    for(int i=0;i<9;i++)
    {
        printf("%c",a[i]);
    }
    printf("\n");
    for(int i=0;i<9;i++)
    {
      printf("%c",b[i]);
    }
      printf("\n");
    for(int i=0;i<9;i++)
    {
      printf("%c",c[i]);
    }
      printf("\n");
    }
