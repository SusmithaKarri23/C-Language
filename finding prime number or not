#include <stdio.h>
int main()
{
   int i,n,flag;
   printf("enter a number : ");
   scanf("%d",&n);
   for(i=2;i<n;i++)
   {
       if(n%i==0)
       {
           printf("not a prime number\n");
           flag=1;
           break;
       }
   }
   if(flag==0)
   {
           printf("prime number\n");
     }
     return 0;
}
